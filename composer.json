{
    "name": "inwave-eu/yii2-wavecms",
    "description": "Yii 2 WaveCMS",
    "keywords": ["yii2", "cms", "cmf", "waveCMS"],
    "homepage": "https://github.com/inwave-eu/yii2-wavecms",
    "type": "project",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/inwave-eu/yii2-wavecms/issues?state=open",
        "wiki": "https://github.com/inwave-eu/yii2-wavecmswiki",
        "source": "https://github.com/inwave-eu/yii2-wavecms"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-bootstrap": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",

        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    },
    "scripts": {
        "post-install-cmd": "php init --env=Development --overwrite=n"
    }
}
